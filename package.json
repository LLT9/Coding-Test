{
  "name": "coding-test",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "node dist/src/index.js",
    "start:dev": "concurrently \"tsc -w -p tsconfig.json\" \"cross-env NODE_ENV=dev nodemon dist/src/index.js\"",
    "start:production": "concurrently \"tsc -w -p tsconfig.json\" \"cross-env NODE_ENV=prod nodemon dist/src/index.js\"",
    "clean": "rimraf coverage dist tmp",
    "prebuild": "npm run lint",
    "build": "tsc -p tsconfig.build.json",
    "build:watch": "tsc -w -p tsconfig.json",
    "lint": "eslint . --ext .ts --ext .mts --fix --ignore-path .gitignore",
    "test": "jest --coverage",
    "prettier": "prettier --config .prettierrc --write .",
    "test:watch": "jest --watch"
  },
